<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <BorderPane prefHeight="741.0" prefWidth="1151.0" stylesheets="@style.css">
         <left>
            <VBox fx:id="leftpanel" prefHeight="623.0" prefWidth="275.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btnCustomers" alignment="BASELINE_LEFT" mnemonicParsing="false" prefHeight="74.0" prefWidth="277.0" text="Customers" textFill="#f8f7f7">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <padding>
                        <Insets left="50.0" />
                     </padding>
                  </Button>
                  <Button fx:id="btnBookings" alignment="BASELINE_LEFT" mnemonicParsing="false" prefHeight="74.0" prefWidth="277.0" text="Bookings" textFill="#f8f7f7">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <padding>
                        <Insets left="50.0" />
                     </padding>
                  </Button>
                  <Button fx:id="btnPackages" alignment="BASELINE_LEFT" mnemonicParsing="false" prefHeight="74.0" prefWidth="277.0" text="Vacation Packages" textFill="#f8f7f7">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <padding>
                        <Insets left="50.0" />
                     </padding>
                  </Button>
                  <Button fx:id="btnProducts" alignment="BASELINE_LEFT" mnemonicParsing="false" onMouseClicked="#loadProducts" prefHeight="74.0" prefWidth="277.0" text="Products" textFill="#f8f7f7">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <padding>
                        <Insets left="50.0" />
                     </padding>
                  </Button>
               </children>
            </VBox>
         </left>
         <top>
            <HBox prefHeight="118.0" prefWidth="1037.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox fx:id="hbox" prefHeight="118.0" prefWidth="346.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="35.0" prefWidth="175.0" text="Travel Experts" textFill="#f5f2f2">
                           <font>
                              <Font name="System Bold" size="24.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="50.0" top="40.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
                  <Button fx:id="btnClose" mnemonicParsing="false" onMouseClicked="#closeWindow" prefHeight="13.0" prefWidth="36.0" text="X" textFill="#f8f6f6">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="840.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </top>
         <center>
            <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <TableView fx:id="tableView" editable="true" layoutX="29.0" layoutY="126.0" prefHeight="421.0" prefWidth="826.0" stylesheets="@style.css">
                    <columns>
                      <TableColumn fx:id="columnOne" minWidth="416.0" prefWidth="416.0" />
                      <TableColumn fx:id="columnTwo" minWidth="416.0" prefWidth="409.0" />
                    </columns>
                  </TableView>
                  <HBox fx:id="header" layoutY="-1.0" prefHeight="74.0" prefWidth="876.0">
                     <children>
                        <Label fx:id="title" prefHeight="97.0" prefWidth="296.0" textFill="#fcfafa">
                           <font>
                              <Font size="24.0" />
                           </font>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                           <padding>
                              <Insets left="20.0" />
                           </padding>
                        </Label>
                     </children>
                  </HBox>
                  <Button fx:id="btnAddProduct" alignment="CENTER" layoutX="323.0" layoutY="559.0" mnemonicParsing="false" onMouseClicked="#addproducts" prefHeight="25.0" prefWidth="77.0" text="Add" textFill="#fcf9f9" />
                  <Button fx:id="btnUpdateProduct" alignment="CENTER" layoutX="474.0" layoutY="559.0" mnemonicParsing="false" onMouseClicked="#updateProducts" prefHeight="25.0" prefWidth="77.0" text="Update" textFill="#f8f7f7" />
               </children>
            </Pane>
         </center>
      </BorderPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
